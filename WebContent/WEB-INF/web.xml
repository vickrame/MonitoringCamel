<?xml version="1.0" encoding="UTF-8"?>
<web-app id="WebApp_ID" version="3.0"
	xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">

	<display-name>Archetype Created Web Application</display-name>

	<servlet>
		<servlet-name>Camel Servlet</servlet-name>
		<servlet-class>com.ujoodha.servlet.StartCamelRouteServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Camel Servlet</servlet-name>
		<url-pattern>StartCamelRouteServlet</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>Camel Servlet AMQP</servlet-name>
		<servlet-class>com.ujoodha.servlet.StartCamelRouteAMQP</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Camel Servlet AMQP</servlet-name>
		<url-pattern>StartCamelRouteAMQP</url-pattern>
	</servlet-mapping>	
	
</web-app>
